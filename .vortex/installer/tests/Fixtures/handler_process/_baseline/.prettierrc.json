{
  "printWidth": 80,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "plugins": ["@homer0/prettier-plugin-jsdoc"],
  "jsdocReplaceTagsSynonyms": false,
  "overrides": [
    {
      "files": ["*.css"],
      "options": {
        "parser": "css",
        "printWidth": 10000,
        "singleQuote": false
      }
    }
  ]
}
