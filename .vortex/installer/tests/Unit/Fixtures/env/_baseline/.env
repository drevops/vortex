# Comment should be preserved
# All possible ways variables can be set in .env files

# Simple unquoted value
SIMPLE_VAR=simple_value

# Quoted value with spaces
QUOTED_VAR="value with spaces"

# Empty value
EMPTY_VAR=

# Value with quotes
QUOTED_CONTENT_VAR="value with \"quotes\""

# Value with equals sign
EQUALS_VAR="key=value"

# Value with special chars
SPECIAL_VAR=!@#$%^&*()

# Value with newlines
MULTILINE_VAR="line1
line2"

# URL value
URL_VAR="https://example.com/path?param=value"

# Leading/trailing spaces
SPACE_VAR=" leading and trailing "

# Numeric value
NUMERIC_VAR=123

# Boolean-like value
BOOL_VAR=true

# Path value
PATH_VAR=/path/to/file

# Email value
EMAIL_VAR=user@domain.com

# Variable to be removed when set to null
REMOVE_VAR=will_be_removed

# End comment
