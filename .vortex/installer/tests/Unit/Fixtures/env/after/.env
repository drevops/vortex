# Comment should be preserved
# All possible ways variables can be set in .env files

# Simple unquoted value
SIMPLE_VAR=new_simple_value

# Quoted value with spaces
QUOTED_VAR="new value with spaces"

# Empty value
EMPTY_VAR=

# Value with quotes
QUOTED_CONTENT_VAR="new value with \"quotes\""

# Value with equals sign
EQUALS_VAR="new key=value"

# Value with special chars
SPECIAL_VAR="new !@#$%^&*()"

# Value with newlines
MULTILINE_VAR="new line1
line2"

# URL value
URL_VAR="new https://example.com/path?param=value"

# Leading/trailing spaces
SPACE_VAR=" new leading and trailing "

# Numeric value
NUMERIC_VAR=new_123

# Boolean-like value
BOOL_VAR=false

# Path value
PATH_VAR="/path/to/new file"

# Email value
EMAIL_VAR="new user@domain.com"

# Variable to be removed when set to null

# End comment
NEW_VAR=new_added_value
